{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "no-const-assign": ["error", "always"],
        "array-callback-return": ["error", "always"],
        "no-dupe-else-if": ["error", "always"],
        "no-dupe-keys": ["error", "always"],
        "no-duplicate-case": ["error", "always"],
        "no-duplicate-imports": ["error", "always"],
        "no-self-compare": ["error", "always"],
        "no-undef": ["error", "always"],
        "no-unused-vars": ["error", "always"],
        // Uncomment in Production
        // "no-console": ["error", "always"],
        "require-await": ["properties", "always"],
        "camelcase": ["properties", "always"]
    }
}